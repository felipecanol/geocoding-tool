{
    "name": "Geocoding",
    "version": "1.0.0",
    "description": "Geocodificar direcciones en Bogot√°",
    "main": "main.js",
    "build": {
        "appId": "geocoding-tool",
        "asar": true,
        "dmg": {
            "contents": [{
                    "x": 110,
                    "y": 150
                },
                {
                    "x": 240,
                    "y": 150,
                    "type": "link",
                    "path": "/Applications"
                }
            ]
        },
        "linux": {
            "target": [
                "AppImage",
                "deb"
            ],
            "icon": "favicon.ico"
        },
        "win": {
            "target": "NSIS",
            "icon": "favicon.ico"
        }
    },
    "scripts": {
        "start": "electron .",
        "pack": "electron-builder --dir",
        "dist": "electron-builder"
    },
    "repository": "https://github.com/felipecanol/geocoding-tool.git",
    "keywords": [
        "geocoding",
        "geocodificar",
        "masivo",
        "masive",
        "geojson"
    ],
    "author": "Felipe Cano <felipecanol@gmail.com> (http://felipe.canol.io/)",
    "license": "GNU Affero General Public License v3.0",
    "devDependencies": {
        "electron": "^29.1.0",
        "electron-builder": "^24.13.3"
    },
    "dependencies": {
        "bootstrap": "^5.2.3"
    }
}